{
  "name": "datocms-plugin-visualize",
  "homepage": "https://github.com/visualize-admin/datocms-plugin-visualize",
  "version": "0.0.1",
  "description": "Create Visualize Charts directly from Dato",
  "scripts": {
    "dev": "vite --port 3333",
    "build": "vite build",
    "addToProject": "yo datocms-plugin:add-to-project",
    "test": "echo 1"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "string-field"
  ],
  "files": [
    "dist",
    "docs"
  ],
  "author": "Jeremy Stucki <jeremy@interactivethings.com>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "Visualize",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.png",
    "entryPoint": "dist/index.html",
    "pluginType": "field_editor",
    "fieldTypes": [
      "string"
    ],
    "parameters": {
      "global": [
        {
          "id": "developmentMode",
          "label": "Development mode",
          "type": "boolean",
          "required": false,
          "default": false,
          "hint": "Shows debug messages in console"
        },
        {
          "id": "visualizeUrl",
          "label": "Visualize URL",
          "type": "string",
          "required": "https://dev.visualize.admin.ch",
          "hint": "URL of the Visualize instance"
        }
      ],
      "instance": []
    }
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "generator-datocms-plugin": "^1.0.24",
    "serve": "^12.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.3",
    "yo": "^4.2.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
